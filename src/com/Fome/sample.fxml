<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>


<VBox prefHeight="389.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Fome.Controller">
   <children>
      <HBox prefHeight="0.0" prefWidth="440.0">
         <children>
            <TextField fx:id="addressBar" onKeyPressed="#onKeyPressed" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#onGo" text="Go" />
            <Button mnemonicParsing="false" onAction="#onBack" text="&lt;" />
            <Button mnemonicParsing="false" onAction="#onForward" text="&gt;" />
         </children>
      </HBox>
      <WebView fx:id="web" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
